             xmlns:local="clr-namespace:Copypasta"
             xmlns:helpers="clr-namespace:Copypasta.Views.Helpers"
             Startup="OnStartup">
    <Application.Resources>
        <helpers:EmptyListToVisibleVisibilityConverter x:Key="EmptyListToVisibleVisibilityConverter" />
